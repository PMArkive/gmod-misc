{
  "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
  "runtime.version": "LuaJIT",
  "runtime.special": {
    "include": "dofile",
    "IncludeCS": "dofile"
  },
  "runtime.nonstandardSymbol": [
    "!",
    "!=",
    "&&",
    "||",
    "//",
    "/**/",
    "continue"
  ],
  "diagnostics.disable": [
    "duplicate-set-field"
  ],
  "workspace.library": [
    "${addons}/garrysmod/module/library"
  ]
}
